{
  "name": "portfolio-website",
  "version": "1.0.0",
  "description": "2023 Portfolio Site",
  "main": "tailwind.config.js",
  "scripts": {
    "tailwind": "npx tailwindcss@3.3.3 -i ./stylesheets/main.css -o ./dist/tailwind.css",
    "autoprefixer": "postcss tailwind.css -u autoprefixer --no-map -o tailwind.css",
    "minify": "postcss tailwind.css -u cssnano --no-map -o tailwind.min.css",
    "build": "npm run tailwind && npm run autoprefixer",
    "production": "npm run tailwind && npm run autoprefixer && npm run minify"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AndrewDESmith/portfolio-2023.git"
  },
  "keywords": [
    "portfolio"
  ],
  "author": "Andrew D.E. Smith",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/AndrewDESmith/portfolio-2023/issues"
  },
  "homepage": "https://github.com/AndrewDESmith/portfolio-2023#readme",
  "dependencies": {
    "tailwindcss": "^3.3.3",
    "tailwindcss-animation-delay": "^1.0.7"
  }
}
